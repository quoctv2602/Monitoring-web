{
  "MyConfig": {
    "ApplicationName": "CallSystemHealth",
    "Version": "1.0.0",
    "ConnectionStrings": "#{MonitoringDB}#",
    "ScheduleCronExpression": "#{WSGetHealthSchedule}#"

  },
  "DiConnectConfig": {
    "ScheduleGetSummaryErrors": "#{ScheduleGetSummaryErrors}#",
    "ScheduleGetTopErrors": "#{ScheduleGetTopErrors}#",
    "TopErrors": "#{TopErrors}#"
  },
  "AppSettings": {
    "WebAPIHelper.MaxRetry": "#{WebAPIHelperMaxRetry}#",
    "WebAPIHelper.ErrorNumbers": "#{WebAPIHelperErrorNumbers}#",
    "DBRetryHelper.MaxRetry": "#{DBRetryHelperMaxRetry}#",
    "DBRetryHelper.TransientErrorNumbers": "#{DBRetryHelperTransientErrorNumbers}#",
    "DBRetryHelper.ConnectionErrorNumbers": "#{DBRetryHelperConnectionErrorNumbers}#"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  }
}
